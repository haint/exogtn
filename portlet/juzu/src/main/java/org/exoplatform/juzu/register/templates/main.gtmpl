<div class="UIFormWithTitle">
	<div class="TitleBar">Create a new account</div>
	<div class="HorizontalLayout">
		<div id="result">
		<% if (flash.error != "") { %>
		<p>
			<strong>${flash.error}</strong>
		</p>
		<% } %>
		<% if (flash.success != "") { %>
		<p>
			<strong>${flash.success}</strong>
		</p>
		<% } %>
		</div>
		
		<div class="FormContainer">
		#{include path=form.gtmpl/}
		</div>
	</div>
</div>